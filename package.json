{
  "name": "osh-starter",
  "version": "0.1.0",
  "description": "Hackathon starter targeting Gemini, Snowflake, DigitalOcean Gradient, and Auth0 prizes",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --ext js src/server.js",
    "dev:client": "cd client && npm run dev",
    "build:client": "cd client && npm run build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-openid-connect": "^2.16.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.0.0",
    "node-fetch": "^2.6.12",
    "pino": "^9.0.0",
    "pino-http": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4",
    "vite": "^7.1.10"
  }
}
